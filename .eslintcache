rom \"../Menu/Menu\";\r\nimport Routing from \"../Routing/Routing\";\r\nimport Footer from \"../Footer/Footer\";\r\nimport NavLog from \"../../UserArea/NavLog/NavLog\";\r\nimport store from \"../../../Redux/Store\";\r\n\r\n\r\n\r\nfunction App() {\r\n    // const state = store.getState().UserReducer.users\r\n    // let logged = null;\r\n    // const unsubscribeMe = store.subscribe(() => logged = store.getState().UserReducer.users.firstName);\r\n\r\n\r\n    return (\r\n\r\n        <BrowserRouter>\r\n            <div className=\"App\">\r\n                <header>\r\n                    <Logo />\r\n                </header>\r\n                <main>\r\n                    <Routing />\r\n                </main>\r\n                <aside>\r\n                    <Menu />\r\n                </aside>\r\n                <footer>\r\n                    <Footer />\r\n                </footer>\r\n            </div>\r\n        </BrowserRouter >\r\n    );\r\n}\r\n\r\n\r\nexport default App;\r\n\r\n","C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\LayoutArea\\Logo\\Logo.tsx",[],"C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\LayoutArea\\Menu\\Menu.tsx",["453","454"],"import React from \"react\";\r\nimport { NavLink } from \"react-router-dom\";\r\nimport \"./Menu.css\";\r\nimport navImage from \"../../../assets/images/logoimages/navImage.png\";\r\nimport Links from '../../CreatingComponents/Links/Links';\r\nimport NavLog from \"../../UserArea/NavLog/NavLog\";\r\n\r\nfunction Menu(): JSX.Element {\r\n    return (\r\n        <div className=\"Menu\">\r\n            <div>\r\n                <Links content={\"דף הבית\"} route={\"/home\"} src={navImage} />\r\n                <Links content={\"השיטה\"} route={\"/method\"} src={navImage} />\r\n                <Links content={\"החנות\"} route={\"/store\"} src={navImage} />\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Menu;\r\n","C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\LayoutArea\\Footer\\Footer.tsx",[],"C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\LayoutArea\\Routing\\Routing.tsx",["455"],"import React from \"react\";\r\nimport { Redirect, Route, Switch } from \"react-router-dom\";\r\nimport Home from \"../../HomeArea/Home\";\r\nimport Page404 from \"../Page404/Page404\";\r\nimport Method from '../../Method/Method'\r\nimport Shop from \"../../ShopArea/Shop/Shop\";\r\nimport HatsItems from \"../../ShopArea/Hats-items/Hats-Items\";\r\nimport AdidasItems from \"../../ShopArea/Adidas-Items/Adidas-Items\";\r\nimport WoolfitItems from \"../../ShopArea/Woolfit-items/Woolfit-items\";\r\nimport Register from '../../UserArea/Register/Register';\r\nimport Login from \"../../UserArea/Login/Login\";\r\n\r\nfunction Routing(): JSX.Element {\r\n    \r\n    return (\r\n        <div className=\"Routing\">\r\n            <Switch>\r\n                <Route path='/home' component={Home} exact />\r\n                <Route path='/method' component={Method} exact />\r\n                <Route path='/store' component={Shop} exact />\r\n                <Route path='/store/hats' component={HatsItems} exact />\r\n                {/* <Route path='/store/adidas-items' component={AdidasItems} exact /> */}\r\n                <Route path='/store/woolfit-items' component={WoolfitItems} exact />\r\n                <Route path='/auth/login' component={Login} exact />\r\n                <Route path='/auth/register' component={Register} exact />\r\n                <Redirect from=\"/\" to=\"/home\" exact />\r\n                <Route component={Page404} />\r\n            </Switch>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Routing;\r\n","C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\HomeArea\\Home.tsx",["456","457","458","459","460"],"import React, { Component } from \"react\";\r\nimport Footer from \"../LayoutArea/Footer/Footer\";\r\nimport Logo from \"../LayoutArea/Logo/Logo\";\r\nimport Menu from \"../LayoutArea/Menu/Menu\";\r\nimport NavLog from \"../UserArea/NavLog/NavLog\";\r\nimport Routing from \"../LayoutArea/Routing/Routing\";\r\nimport \"./Home.css\";\r\n\r\n\r\n//img with class\r\nclass Home extends Component {\r\n    public render(): JSX.Element {\r\n        return (\r\n            <div className=\"Home p-col-11 p-md-11 p-lg-12\">\r\n                <h1 className=\"MainSentence\">\r\n                    <section>\r\n                        הינה שיטת אימון ומותג המשלבת WoolFit\r\n                    </section>\r\n                    <section>\r\n                        מספר תחומי ספורט שונים ומקצועיים הפונים לכלל\r\n                    </section>\r\n                    <section>\r\n                        האוכלוסיה אשר גורמים משפרים את איכות החיים שלנו ועושה את האימון מהנה יותר\r\n                    </section>\r\n                </h1>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n// class Home extends Component {\r\n//     public render(): JSX.Element {\r\n//         return (\r\n//             <div className=\"Home\">\r\n//                 <header>\r\n//                     <Logo />\r\n//                 </header>\r\n//                 <main>\r\n\r\n//                 </main>\r\n//                 <aside>\r\n//                     <Menu />\r\n//                 </aside>\r\n//                 <footer>\r\n//                     <Footer />\r\n//                 </footer>\r\n//             </div>\r\n//         );\r\n//     }\r\n// }\r\n\r\nexport default Home;\r\n\r\n","C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\Method\\Method.tsx",["461"],"import React from 'react';\r\nimport Links from '../CreatingComponents/Links/Links';\r\nimport './Method.css';\r\nimport navImage from '../../assets/images/logoimages/navImage.png';\r\nimport NavLog from '../UserArea/NavLog/NavLog';\r\n\r\nfunction Method(): JSX.Element {\r\n    return (\r\n        <div className=\"Method p-col-12 p-md-11 p-lg-12\">\r\n            <section>\r\n                <p>\r\n                    מזה WoolFit?\r\n                    WoolFit הינה שיטת אימון שאני מפתח כבר כמה חודשים.\r\n                    מעל 20 שנה שאני עוסק בהמון תחומים שונים של ספורט תחרותי ומאתגר\r\n                    כגון: כדורסל, כדורגל, אתלטיקה, נינגה, קליסטניק (משקלי גוף), סוגי הגלישה למיניהם ועוד המון סוגי ספורט שונים\r\n                    ומהנים החלטתי ללמוד קורס מדריכי כושר בקמפוס שיאים .\r\n                    את הקורס סיימתי בדצמבר 2019 בהצטיינות.\r\n                </p>\r\n                <p>\r\n                    לאחר שלמדתי את עקרונות האימון והכושר הגופני הבסיסיים , החלטתי לשלב בין עולמות הספורט שאני חובב לבין עקרונות\r\n                    הכושר שלמדתי ובכך לפתח שיטת אימון חדשנית שהופכת את האימון הכושר הרגיל שאתם מכירים לאימון מאתגר מהנה ומשפר\r\n                    יותר.\r\n                </p>\r\n                <p>\r\n                    למה משפר? ככל שהגוף שלכם והשרירים שלכם יזוזו ויפעלו בצורות שונות והתנגדויות שונות\r\n                    אנחנו נגרה את היחידות המוטוריות בשרירים יותר מכל אימון רגיל , נפתח קורדינאציה תוך ובין שרירית , נשרוף קלוריות\r\n                    נשפר את הכושר האירובי שלנו – וכמובן נהנה מהדרך.\r\n                </p>\r\n                <p>\r\n                    היא פונה לקהל הרחב – מילדים נשים גברים עם ובלי נסיון ספורטיבי אשר מעוניינים\r\n                    ,מאתגרת ומשפרת הרבה יותר.\r\n                    מה בתוכנית?\r\n                    באימונים נשמור נקפיד על הטכניקה ונשלב את כל עקרונות הכושר הגופניים עם נגיעות מעולמות הכושר השונים ,\r\n                    משקל גוף - עמידות ידיים , קליסטניק ונינגה\r\n                    יציבה וליבה חזקים – בעזרת תרגילים מגוונים מעולמות שונים נשפר את השווי משקל היציבות ושרירי הליבה .\r\n                    ניגע ונשלב את עולם הכדורים – כדורגל , כדורסל , טניס בעזרתם נשפר את הקורדינציה ונשלב עוד המון תרגילים בנאלים\r\n                    שיהפכו למאתגרים ומהנים הרבה יותר.\r\n                </p>\r\n            </section>\r\n            <div className=\"navBars\">\r\n                <Links src={navImage} route={\"/videos\"} content={\"מדיה\"} />\r\n                <Links src={navImage} route={\"/trainers\"} content={\"מורקים\"} />\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Method;","C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\LayoutArea\\Page404\\Page404.tsx",[],"C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\UserArea\\Register\\Register.tsx",[],"C:\\Programs\\WoolfitProject\\WoolfitWeb\\woolfit-frontend\\src\\Components\\UserArea\\NavLog\\NavLog.tsx",["462"],"import React, { useState } from \"react\";\nimport \"./NavLog.css\";\nimport { NavLink } from \"react-router-dom\";\nimport { UserActionType, userModel } from \"../../../Redux/UserState\";\nimport store from \"../../../Redux/Store\";\nimport { Unsubscribe } from \"redux\";\n\nfunction NavLog(): JSX.Element {\n\n    const [users, setUsers] = useState<userModel>(store.getState().UserReducer.users);\n\n    const unSubscribe: Unsubscribe = store.subscribe(() => {\n        const users = store.getState().UserReducer.users;\n        setUsers(users);\n    })\n\n\n    function logoutUser() {\n        const action = { type: UserActionType.UserLoggedOut, payload: null };\n        store.dispatch(action);\n    }\n\n    const login =\n        <>\n            <NavLink to=\"/auth/login\" >התחברות</NavLink>\n            <span> | </span>\n            <NavLink to=\"/auth/register\">הרשמה</NavLink>\n        </>;\n\n    const logout =\n        <>\n            <NavLink to=\"/\" onClick={() => logoutUser()}>התנתקות</NavLink>\n            <span> | </span>\n            <span>  ברוך הבא {users.user && users.user.firstName}</span>\n        </>;\n\n    return (\n        <nav className=\"NavLog\">\n            {users.isLoggedIn ? logout : login}\n        </nav>\n    );\n    unSubscribe();\n}\n\nexport default NavLog;\n","C:\\Programs\\WoolfitProjec